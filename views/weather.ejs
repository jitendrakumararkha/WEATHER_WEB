    <div class="container-fluid main_header">
      <div class="row">
        <div class="col-md-10 col-12 mx-auto">
          <div class="main_content">
            <form class="temp_form "  action="">
              <input type="text" id="cityName" placeholder="cityName" >
              <input type="submit"  value="search"  id="subbtn"></form> </div>
              <div class="tempinformation" >
                <div class="top_layer">
                  <p id="day">Tuesday </p>
                  <p id="today_date"> 20 OCT</p>
                </div>
                <div class="mainlayer">
               <p id="city_name">Get output here</p>
               <div class="middle_layer data_hide">
                 <p id="temp"><span>0</span><sup>o</sup>C</p>
                 <p  id="temp_status"> <i class="fa fa-cloud" aria-hidden="true"></i></p>
               </div>
                </div>
              </div>
            
          <div class="row">
            <div class="col-md-6 col-12 main_header_left">
              
              </div>
